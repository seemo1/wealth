CREATE TABLE `social_coin_record` (
  `coin_id` varchar(20) COLLATE utf8_unicode_ci NOT NULL,
  `coin_number` int(11) DEFAULT '0',
  `msg` text COLLATE utf8_unicode_ci,
  `status` varchar(4) COLLATE utf8_unicode_ci DEFAULT NULL,
  `type` varchar(3) COLLATE utf8_unicode_ci DEFAULT NULL,
  `user_id` varchar(40) COLLATE utf8_unicode_ci DEFAULT NULL,
  `transact_id` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL,
  `gae_coin_id` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL,
  `publish_time` timestamp NULL DEFAULT NULL,
  `last_update_time` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`coin_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;


CREATE TABLE `social_user_account` (
  `user_id` varchar(40) COLLATE utf8_unicode_ci NOT NULL,
  `coins` int(11) DEFAULT NULL,
  `fuel` int(11) DEFAULT NULL,
  `publish_time` timestamp NULL DEFAULT NULL,
  `last_update_time` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`user_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
